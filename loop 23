#include <stdio.h>
int main() {
    int n, i, j, count = 0;
    int isPrime;
    printf("Enter a number: ");
    scanf("%d", &n);
    for (i = 2; i <= n; i++) {
        isPrime = 1;
        for (j = 2; j <= i / 2; j++) {
            if (i % j == 0) {
                isPrime = 0;
                break;
            }
        }
        if (isPrime == 1) {
            count++;
        }
    }
    printf("Total prime numbers between 1 and %d = %d", n, count);
    return 0;
}
